{
  "disabled": false,
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "$return",
      "type": "sendGrid",
      "direction": "out",
      "apiKey" : "MySendGridKey",
      "to": "{ToEmail}",
      "from": "{FromEmail}",
      "subject": "SendGrid output bindings"
    }    
  ]
}